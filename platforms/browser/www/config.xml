<?xml version='1.0' encoding='utf-8'?>
<widget id="org.gospelsounders.allSDAHymnals" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>All SDA Hymnals</name>
    <description>
        All Seventh-day Adventist Hymnals in history.
    </description>
    <author email="surgbc@gmail.com" href="https://github.com/GospelSounders/all-sda-hymnals">
        Brian Onang'o
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="AndroidExtraFilesystems" value="files,cache, sdcard, cache-external, files-external" />
</widget>
